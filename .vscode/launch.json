  "version": "0.2.0",
  "configurations": [
    {
      "name": "Train",
      "type": "python",
      "request": "launch",
      "module": "engine.train",
      "args": [
        "--model_save",
        "output/models/rgb-test/",
        "--detection_dir",
        "./data/segmentation_results",
        "--dataset_dir",
        "./data"
      ],
      "justMyCode": true
    },
    {
      "name": "Test",
      "type": "python",
      "request": "launch",
      "module": "evaluation.evaluate",
      "args": [
        "--model_save",
        "output/test",
        "--resume",
        "1",
        "--resume_model",
        "./output/models/rgb-direct/model_09.pth",
        "--eval_seed",
        "1677483078",
        "--detection_dir",
        "./data/segmentation_results",
        "--dataset_dir",
        "./data"
      ],
      "justMyCode": true
    },
    {
      "name": "Viz",
      "type": "python",
      "request": "launch",
      "module": "evaluation.evaluate_viz",
      "args": [
        "--model_save",
        "output/test",
        "--resume",
        "1",
        "--resume_model",
        "./model_119.pth",
        "--eval_seed",
        "1677483078",
        "--detection_dir",
        "./data/segmentation_results",
        "--dataset_dir",
        "./data"
      ],
      "justMyCode": true
    },
    {
      "name": "None",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false
    }
  ]
}
